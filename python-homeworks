name = input("enter Student name: ")
last_name = input("enter Student last name: ")
grade1 = float(input("enter Student grade 1: "))
grade2 = float(input("enter Student grade 2: "))
grade3 = float(input("enter Student grade 3: "))
average = (grade1+grade2+grade3)/3

if average >= 10:
    status = "Passed"
else:
    status = "Failed"

print(f"{name} {last_name} with an average grade of {average} in the field of ... is {status}.")
