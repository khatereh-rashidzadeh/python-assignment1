a = float(input("Enter the length of side a: "))
b = float(input("Enter the length of side b: "))
c = float(input("Enter the length of side c: "))

if (a + b > c) and (a + c > b) and (b + c > a):
    print("Yes, it is possible to form a triangle with sides of length a =", a, ", b =", b, ", and c =", c)
else:
    print("No, it is not possible to form a triangle with sides of length a =", a, ", b =", b, ", and c =", c)
